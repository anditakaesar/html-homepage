doctype html
html
    head
        include includes/main-header.pug
        title Main Index
    body#home
        include includes/main-navbar.pug

        // bigtitle
        .main-container
            .main-container-info.uk-flex.uk-flex-center
                .main-profile-info.main-profile-left.uk-card.uk-card-body #{lorem2}
                .main-profile-pic.uk-card.uk-card-hover.uk-margin-medium-left
                    img(src="img/profile.png")
                .main-profile-info.main-profile-right.uk-card.uk-card-body.uk-margin-medium-left
                    p.main-profile-name #{myname}
                    a(href="#")
                        img(src="img/linkedin.svg")
                    a(href="#", class="uk-margin-small-left")
                        img(src="img/github.svg")
                    a(href="#", class="uk-margin-small-left")
                        img(src="img/facebook.svg")
                    a(href="#", class="uk-margin-small-left")
                        img(src="img/twitter.svg")
                    a(href="#", class="uk-margin-small-left")
                        img(src="img/telegram.svg")

        // myworks
        .second-container#myworks
            div(uk-slider="center: true")
                .uk-position-relative.uk-visible-toggle.uk-dark(tabindex="-1")
                    ul(class="uk-slider-items uk-child-width-1-2@s uk-grid")
                        li
                            .uk-card.uk-card-default
                                .uk-card-media-top
                                    img(src="img/examples/image1.jpg", alt="")
                                .uk-card-body
                                    h3.uk-card-title Headline
                                    p #{lorem2}
